{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#0077c2",
          "style": "plain",
          "template": "╭─",
          "type": "text"
        },
        {
          "background": "#2343e2",
          "foreground": "#30cc7d",
          "properties": {
            "style": "roundrock",
            "always_enabled": true
          },
          "leading_diamond": "",
          "style": "diamond",
          "template": "󰦖{{.PromptCount}}@{{regexReplaceAll \" \" .FormattedMs \"\"}}",
          "type": "executiontime"
        },
        {
          "foreground": "#000000",
          "background": "#91f2ff",
          "style": "diamond",
          "template": "<#2343e2,#91f2ff></>{{.Name}} {{upper .Version}}",
          "type": "shell",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "PowerShell"
            }
          }
        },
        {
          "background": "#cfee45",
          "foreground": "#000000",
          "leading_diamond": "<#91f2ff,#cfee45></>",
          "properties": {
            "style": "full"
          },
          "style": "diamond",
          "template": "{{.Path}}",
          "trailing_diamond": "",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#addb67",
          "foreground": "#193549",
          "leading_diamond": "",
          "style": "diamond",
          "trailing_diamond": "",
          "background_templates": [
            "{{if or (.Working.Changed) (.Staging.Changed)}}#ffeb3b{{end}}",
            "{{if and (gt .Ahead 0) (gt .Behind 0)}}#f78c6c{{end}}",
            "{{if gt .Ahead 0}}#21c7a8{{end}}",
            "{{if gt .Behind 0}}#c792ea{{end}}"
          ],
          "template": "{{regexReplaceAll \" \" .BranchStatus \"\"}}{{if .Working.Changed}}|{{regexReplaceAll \" \" .Working.String \"\"}}{{end}}{{if .Staging.Changed}}|{{regexReplaceAll \" \" .Staging.String \"\"}}{{end}}{{if gt .StashCount 0}}|{{.StashCount}}{{end}}{{.Ref}}@{{regexReplaceAll \".*/\" .RepoName \"\"}}{{if gt .WorktreeCount 0}}{{.WorktreeCount}}{{end}}{{if eq .UpstreamIcon \"\"}}{{else}}{{.UpstreamIcon}}{{end}}",
          "properties": {
            "branch_icon": "",
            "git_icon": "",
            "github_icon": "",
            "azure_devops_icon": "ﴃ",
            "branch_ahead_icon": "",
            "branch_behind_icon": "",
            "branch_gone_icon": "≢",
            "branch_identical_icon": "≡",
            "tag_icon": "",
            "commit_icon": "",
            "merge_icon": "",
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#0077c2",
          "style": "plain",
          "template": "│",
          "type": "text"
        },
        {
          "foreground": "#1865f5",
          "properties": {
            "arch": "",
            "windows": "󰍲",
            "wsl": "\ue712",
            "wsl_separator": " on "
          },
          "style": "diamond",
          "template": "{{.Icon}}",
          "trailing_diamond": "",
          "type": "os"
        },
        {
          "background": "#8f43f3",
          "foreground": "#cfcf9f",
          "leading_diamond": "",
          "style": "diamond",
          "template": "󰍛{{regexReplaceAll \"\\\\([^\\\\)]*\\\\)\" (regexFind \"(Microsoft|Intel|AMD).*\" (trim (split \"@\" (index .CPU 0).ModelName)._0)) \"\"}}@{{(index .CPU 0).Mhz}}Mhz|{{(div (sub .PhysicalTotalMemory .PhysicalAvailableMemory) 1048576.0)}}[+{{(div (sub .SwapTotalMemory .SwapFreeMemory) 1048576)}}]/{{(div .PhysicalTotalMemory 1048576)}}[+{{div .SwapTotalMemory 1048576}}]MiB",
          "type": "sysinfo"
        },
        {
          "background": "#ff7a33",
          "foreground": "#000000",
          "leading_diamond": "<#8f43f3,#ff7a33></>",
          "trailing_diamond": "",
          "properties": {
            "time_format": "15:04:05@Jan02Mon"
          },
          "style": "diamond",
          "template": "{{upper (.CurrentDate | date .Format)}}󱛡",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "dotnet",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#193549",
          "background": "#7fffff",
          "template": ".NET {{upper .Full}}󰪮"
        },
        {
          "type": "rust",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#193549",
          "background": "#99908a",
          "template": "Rust {{upper .Full}}-{{(split \" \" .BuildMetadata)._0}}"
        },
        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#193549",
          "background": "#99908a",
          "template": "Python {{upper .Full}}"
        },
        {
          "type": "go",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#193549",
          "background": "#7FD5EA",
          "template": "Go {{upper .Full}}󰟓"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#0077c2",
          "style": "plain",
          "template": "╰─",
          "type": "text"
        },
        {
          "type": "session",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#3c000e",
          "background": "#c3f6f1",
          "template": "{{if .SSHSession}}󰀑{{else}}󰀄{{end}}{{.UserName}}@{{.HostName}}{{if .WSL}}[@WSL]{{else}}{{end}}"
        },
        {
          "type": "exit",
          "style": "diamond",
          "foreground": "#91f2ff",
          "background_templates": [
            "{{if gt .Code 0}}#e91e1b{{else}}#16e11b{{end}}"
          ],
          "trailing_diamond": "",
          "template": "{{if .Root}}<#ffff00>{{else}}<#0000ff>󰞷{{end}}</>",
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "networks",
          "style": "diamond",
          "background": "#CFFF7F",
          "foreground": "#222222",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "SSIDAbbr": 7,
            "IconAsAT": true,
            "ShowType": false,
            "LinkSpeedUnit": "A"
          },
          "IconAsAT": true,
          "template": "{{if eq .Status \"Connected\"}}{{.Networks}}{{else}}󰪎{{end}}"
        },
        {
          "type": "battery",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "transparent",
          "background": "#CBEB3B",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ],
          "template": "{{if .Error}}󰂃{{else}}{{.Percentage}}%{{if eq \"Charging\" .State.String}}{{if lt .Percentage 21}}󰂆{{else if lt .Percentage 33}}󰂇{{else if lt .Percentage 45}}󰂈{{else if lt .Percentage 57}}󰂉{{else if lt .Percentage 69}}󰂊{{else if lt .Percentage 81}}󰂋{{else if lt .Percentage 93}}󰂅{{else}}󰂄{{end}}{{else if or (eq \"Discharging\" .State.String) (eq \"Full\" .State.String)}}{{if lt .Percentage 21}}󰂎{{else if lt .Percentage 29}}󰁺{{else if lt .Percentage 37}}󰁻{{else if lt .Percentage 45}}󰁼{{else if lt .Percentage 53}}󰁽{{else if lt .Percentage 61}}󰁾{{else if lt .Percentage 69}}󰁿{{else if lt .Percentage 77}}󰂀{{else if lt .Percentage 85}}󰂁{{else if lt .Percentage 93}}󰂂{{else}}󰁹{{end}}{{else if eq \"Unknown\" .State.String}}󰂑{{else}}󰂃{{end}}{{end}}",
          "properties": {
            "display_error": true
          }
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{.Folder}}",
  "osc99": true,
  // "transient_prompt": {
  //   "background": "transparent",
  //   "foreground": "#FEF5ED",
  //   "template": "{{if .WSL}}\ue712 {{else}}{{end}}{{if .Root}}\uF0E7 {{else}}{{end}} "
  // },
  "version": 2
}
